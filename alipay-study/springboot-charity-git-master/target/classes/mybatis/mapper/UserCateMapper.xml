<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace=绑定一个对应的Dao/Mapper接口-->
<mapper namespace="com.hhb.mapper.UserCateMapper">
    <select id="getCateById" resultType="com.hhb.pojo.User_category">
        select * from user_category where cate_id=#{id}
    </select>

</mapper>