<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.test.car.dao.BrandMapper">
<!--    <cache eviction="FIFO" flushInterval="60000" readOnly="false" size="1024"/>-->
    <select id="listByCompanyId" resultType="org.test.car.model.vo.BrandVO">
        select *
        from brand
        where company_id=#{companyId}
    </select>

    <select id="getBrand" resultType="org.test.car.model.dom.BrandDO">
        select *
        from brand
        where id=#{id}
    </select>
</mapper>